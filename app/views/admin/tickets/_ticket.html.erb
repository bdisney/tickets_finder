<tr>
  <td><%= ticket.user.email %></td>
  <td><%= ticket.train.number %></td>
  <td><%= ticket.train.route.title %></td>
  <td><%= ticket.start_station.title %></td>
  <td><%= ticket.last_station.title %></td>
  <td><%= render 'shared/actions', links: {
      show:   admin_ticket_path(ticket),
      edit:   edit_admin_ticket_path(ticket),
      delete: admin_ticket_path(ticket)
  } %></td>
</tr>